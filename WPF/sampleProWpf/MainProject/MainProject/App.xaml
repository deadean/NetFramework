<Application x:Class="MainProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml"
             >
    <!--StartupUri="/MainProject;component/view/WindowsPagesRichControl/Page1.xaml"-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dictionary1.xaml"/>
                <ResourceDictionary Source="Resources/Brushes.xaml"/>
                <ResourceDictionary Source="Resources/Buttons.xaml"/>
                <ResourceDictionary Source="Resources/Styles.xaml"/>
                <ResourceDictionary Source="Resources/Theme1.xaml"/>
                <ResourceDictionary Source="Resources/generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
    <JumpList.JumpList>
        <JumpList ShowRecentCategory="True">
            <JumpTask CustomCategory="Other Programs" Title="NotePad" Description="Open a sample document"
                      ApplicationPath="c:windows/notepad.exe"
                      IconResourcePath="c:windows/notepad.exe"/>
        </JumpList>
        
    </JumpList.JumpList>
    
</Application>
