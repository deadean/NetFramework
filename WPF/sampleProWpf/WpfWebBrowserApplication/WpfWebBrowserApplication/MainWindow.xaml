<Window x:Class="WpfWebBrowserApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
		<StackPanel Orientation="Horizontal">
			<Button Width="100" Height="50" Click="Button_Click">Navigate</Button>
			<Button Width="100" Height="50" Click="Button_Click_1">Show Content</Button>
		</StackPanel>
			
		<WebBrowser x:Name="browser" Height="800">
			
		</WebBrowser>
	</StackPanel>
</Window>
